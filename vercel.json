{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "rewrites": [
    { "source": "/services", "destination": "/services.html" },
    { "source": "/our-work", "destination": "/our-work.html" },
    { "source": "/about-us", "destination": "/about-us.html" },
    { "source": "/contact-us", "destination": "/contact-us.html" }
  ]
}
